{
    "name": "teelaravel/theme",
    "description": "Theme feature for Laravel",
    "keywords": ["laravel", "theme", "layout", "asset", "widget"],
    "homepage": "https://github.com/teelang/laravel-theme",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tricky",
            "email": "tricky@gvr.vn"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "illuminate/support": ">=5.0.1"
    },
    "autoload": {
        "psr-4": {
            "TeeLaravel\\Theme\\": "src/"
        },
        "classmap": [
            "src/Exceptions.php"
        ],
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TeeLaravel\\Theme\\ThemeServiceProvider"
            ]
        }
    }
}